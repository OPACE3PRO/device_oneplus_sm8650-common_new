        <name>vendor.oplus.hardware.touch</name>
        <version>2</version>
        <interface>
            <name>IOplusTouch</name>
            <instance>default</instance>
        </interface>
    </hal>
    <hal format="aidl" optional="true">       
        <name>android.hardware.biometrics.fingerprint</name>
        <interface>
