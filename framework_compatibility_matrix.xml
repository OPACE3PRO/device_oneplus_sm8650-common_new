        </interface>
    </hal>
    <hal format="aidl" optional="true">
        <name>android.hardware.biometrics.fingerprint</name>
        <interface>
            <name>IBiometricsFingerprint</name>
            <instance>oplus</instance>
        </interface>
    </hal>
